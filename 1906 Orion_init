function addDynamicCSS(cssText) {
  const style = document.createElement('style');
  style.type = 'text/css';
  style.appendChild(document.createTextNode(cssText));
  document.head.appendChild(style);
}

addDynamicCSS(`
  #question_form {
    color: #062c42;
  }

